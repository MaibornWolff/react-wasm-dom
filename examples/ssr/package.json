{
  "author": "Mario Reder <mario.reder@maibornwolff.de",
  "name": "react-wasm-ssr",
  "version": "0.0.1",
  "scripts": {
    "start": "npm-run-all --parallel build:dev watch",
    "build": "webpack -p --progress --colors",
    "build:dev": "webpack -d --watch --progress --colors",
    "watch": "nodemon ./dist/server --watch dist/server -e js,ts,jsx,tsx,json,wasm,html"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-typescript": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@wasm-tool/wasm-pack-plugin": "^1",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  }
}
