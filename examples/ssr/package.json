{
  "author": "Mario Reder <mario.reder@maibornwolff.de",
  "name": "react-wasm-ssr",
  "version": "0.0.1",
  "scripts": {
    "start": "npm-run-all --parallel build:dev watch",
    "build": "webpack --mode production --progress",
    "build:dev": "webpack --mode development --watch --progress",
    "watch": "nodemon ./dist/server --watch dist/server -e js,ts,jsx,tsx,json,wasm,html"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16",
    "react-dom": "^16",
    "react-is": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-typescript": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/express": "^4.17.1",
    "@types/node": "^15",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@types/react-is": "^16.7.1",
    "@wasm-tool/wasm-pack-plugin": "^1",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^5.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.4.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^2.5.2"
  }
}
